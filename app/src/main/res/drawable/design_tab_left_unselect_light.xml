        <shape>
            <stroke android:width="0.5dp" android:color="@color/colorPrimary" />

            <solid android:color="@color/transparent" />

            <corners android:bottomLeftRadius="@dimen/radius" android:topLeftRadius="@dimen/radius" />
        </shape>
    </item>

</selector>
